{
  "tasks": {
    "stop": "pm2 stop nodeschecker",
    "start": "pm2 start --interpreter='deno' --interpreter-args='run --allow-net --allow-read --allow-env --allow-run --allow-write' --name nodeschecker index.ts && pm2 logs nodeschecker",
    "logs": "pm2 logs nodeschecker",
    "reload": "pm2 reload nodeschecker && pm2 logs nodeschecker",

    "run": "deno run --allow-net --allow-read --allow-env --allow-run --allow-write index.ts",
    "watch": "deno run --watch --allow-net --allow-read --allow-env --allow-run --allow-write index.ts"
  },
  "imports": {
    "sort-es": "https://esm.sh/sort-es@1.6.11/",
    "grammy/": "https://deno.land/x/grammy@v1.16.2/",
    "axiod": "https://deno.land/x/axiod@0.26.2/mod.ts",
    "sleep": "https://deno.land/x/sleep@v1.2.1/mod.ts",
    "join": "https://deno.land/std@0.192.0/path/mod.ts",
    "escapeHtml": "https://deno.land/x/escape_html@1.0.0/mod.ts",
    "duplicatedFilesCleanerIncognito": "https://deno.land/x/duplicated_files_cleaner_incognito@1.2/mod.ts"
  }
}
